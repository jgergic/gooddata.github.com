ALTER ATTRIBUTE {attr.opportunity.id} ADD LABELS {label.opportunity.id} VISUAL(TITLE "Opportunity ID") AS {f_opportunity.nm_id}; 
ALTER ATTRIBUTE {attr.opportunitysnapshot.id} ADD LABELS {label.opportunitysnapshot.id} VISUAL(TITLE "Opportunity Snapshot ID") AS {f_opportunitysnapshot.nm_id}; 
